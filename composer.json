{
    "name": "teikun-86/anti-spoof",
    "version": "0.1.0",
    "description": "Laravel middleware to detect and prevent spoofed IPs, forged headers, and identity manipulation attempts.",
    "type": "library",
    "require": {
        "lorisleiva/laravel-actions": "^2.9",
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.4",
        "phpunit/phpunit": "^11.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teikun86\\AntiSpoof\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teikun86\\AntiSpoof\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aziz Febriyanto",
            "email": "azizfebriyanto12@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Teikun86\\AntiSpoof\\AntiSpoofServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "security",
        "spoof",
        "middleware",
        "anti-spoof",
        "ip-protection",
        "user-agent"
    ]
}
